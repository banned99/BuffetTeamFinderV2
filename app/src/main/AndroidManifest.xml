<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ggwp.caliver.banned.buffetteamfinderv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.buffet.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.buffet.activities.SearchActivity" />
        <activity android:name="com.buffet.activities.ViewMapActivity" />
        <activity android:name="com.buffet.activities.ViewNotiActivity" />
        <activity android:name="com.buffet.activities.EditProfileActivity"></activity>
    </application>

</manifest>